{
  "name": "node-web-audio-api",
  "version": "0.2.1",
  "author": "Benjamin Matuszewski",
  "description": "Node.js bindings for web-audio-api-rs using napi-rs",
  "main": "index.js",
  "repository": "git@github.com:b-ma/node-web-audio-api.git",
  "license": "BSD-3-Clause",
  "keywords": [
    "audio",
    "web audio api",
    "rust",
    "n-api"
  ],
  "engines": {
    "node": ">= 14"
  },
  "napi": {
    "name": "node-web-audio-api"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "scripts": {
    "artifacts": "napi artifacts",
    "build": "napi build --platform --release",
    "build:debug": "napi build --platform",
    "check": "cargo fmt && cargo clippy",
    "generate": "node generator/index.js",
    "preversion": "npm run generate"
  },
  "devDependencies": {
    "chalk": "^5.0.1",
    "dotenv": "^16.0.2",
    "node-ssh": "^13.0.0",
    "octokit": "^2.0.7",
    "ping": "^0.4.2"
  },
  "dependencies": {
    "@napi-rs/cli": "^2.10.0",
    "@node-rs/helper": "^1.3.3"
  }
}
